# ==== Purpose ====
#
# verify that slave_max_allowed_packet works

source include/master-slave.inc;
source include/have_binlog_format_statement.inc;

call mtr.add_suppression("Slave I/O: Got fatal error 1236 from master when reading data from binary log:");

--disable_warnings
drop database if exists foobar;
--enable_warnings

connection slave;
source include/stop_slave.inc;

connection master;
create database foobar;
use foobar;
create table `t1` (`f1` LONGTEXT) ENGINE=MyISAM;

# This should fail with max_allowed_packet too small
INSERT INTO `t1`(`f1`) VALUES ('');

SET @@global.max_allowed_packet=8192;
SET @@global.net_buffer_length=8192;

# Reconnect to master for new setting to take effect
disconnect master;
connect (master,localhost,root,,foobar);

# This should succeed
INSERT INTO `t1`(`f1`) VALUES ('');

SET @@global.max_allowed_packet=1024;
SET @@global.net_buffer_length=1024;

# Reconnect to master for new setting to take effect
disconnect master;
connect (master,localhost,root,,foobar);

connection slave;
let $slave_timeout= 20;
source include/start_slave.inc;
sleep 1.0;
SHOW STATUS LIKE 'Slave_running';
select count(*) from foobar.`t1` /* must be 1 */;
select length(f1) from foobar.t1;

connection master;
SET @@global.slave_max_allowed_packet=8192;
save_master_pos;

connection slave;
SET @@global.slave_max_allowed_packet=8192;
source include/stop_slave.inc;
source include/start_slave.inc;
sync_with_master;
SHOW STATUS LIKE 'Slave_running';
select count(*) from foobar.`t1` /* must be 1 */;
select length(f1) from foobar.t1;

connection master;
drop database foobar;
save_master_pos;

connection slave;
sync_with_master;

--echo ==== clean up ====
connection master;
SET @@global.slave_max_allowed_packet=0;
SET @@global.max_allowed_packet=8192;
SET @@global.net_buffer_length=16384;

# slave is stopped
connection slave;
SET @@global.slave_max_allowed_packet=0;

# End of tests
