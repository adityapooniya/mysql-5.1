# stress tests: blobs=no, crash=no, compress=yes

# Don't test this under valgrind, memory leaks will occur
--source include/not_valgrind.inc
--source include/have_innodb.inc
--source include/master-slave.inc
--source include/big_test.inc

--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings

# create the actual table
CREATE TABLE t1(id INT AUTO_INCREMENT PRIMARY KEY,
                msg VARCHAR(255),
                KEY msg_i(msg))
ENGINE=INNODB
ROW_FORMAT=COMPRESSED
KEY_BLOCK_SIZE=1;

let $use_blob=0;
let $do_crash=0;
let $do_compress=1;

--let $num_crashes = 1
--let $num_workers = 10
--let $num_transactions = 5000 
--let $kill_db_after = 0

--source suite/innodb_plugin/include/innodb_stress.inc
select variable_value > 0 from information_schema.global_status where variable_name='Binlog_fsync_grouped';
