# stress tests: blobs=no, crash=yes, compress=no

# Don't test this under valgrind, memory leaks will occur
--source include/not_valgrind.inc
--source include/have_innodb.inc
--source include/master-slave.inc
--source include/big_test.inc

# Don't test in Debug mode, for now.  See Task #1067014
--source include/have_nodebug.inc

--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings

# create the actual table
CREATE TABLE t1(id INT AUTO_INCREMENT PRIMARY KEY,
                msg VARCHAR(255),
                KEY msg_i(msg))
ENGINE=INNODB;

let $use_blob=0;
let $do_crash=1;
let $do_compress=0;

--let $num_crashes = 5
--let $num_workers = 20
--let $num_transactions = 0
--let $kill_db_after = 600
--let $num_records = 100000
--let $max_rows = 200000

--source suite/innodb_plugin/include/innodb_stress.inc
