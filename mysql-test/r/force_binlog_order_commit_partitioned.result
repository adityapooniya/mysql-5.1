DROP TABLE IF EXISTS t1;
CREATE TABLE t1 (k INT NOT NULL, a INT NOT NULL, b INT NOT NULL, c INT NOT NULL, PRIMARY KEY(k)) ENGINE=InnoDB
partition by range(k) (
partition part0 values less than (0),
partition part1 values less than (1),
partition part2 values less than (2),
partition part3 values less than (3)
);
